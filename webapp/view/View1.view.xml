<mvc:View controllerName="zso.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
      <VBox class="sapUiTinyMargin">
        <Label text="Last Sales Order created"/>
        <Input text="" id="lastOrderId" editable="true"/>

        <TextArea id="textArea1" value="" rows="8" width="100%" editable="false"/>
      </VBox>
      <HBox class="sapUiTinyMargin">
        <Button text="Create Sales Order" press="onCreateSOHeader"/>
        <Button text="Create Sales Order (Deep)" press="onCreateDeepSOHeader"/>
        <Button text="Read Sales Order" press="onReadSOHeader"/>
        <Button text="Upadate Sales Order" press="onUpdateSOHeader"/>
        <Button text="Delete Sales Order" press="onDeleteSOHeader"/>
        <Button text="Read entitySet" press="onReadEntitySet"/>
      </HBox>
    </Page>
</mvc:View>